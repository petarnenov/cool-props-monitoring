{
  "name": "cool-props-monitoring",
  "description": "A cool props monitoring tool for React",
  "author": {
    "name": "@petarnenov",
    "email": "patarnenovpetrov@gmail.com"
  },
  "private": false,
  "version": "0.0.6",
  "type": "module",
  "main": "dist/main.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npm run build && vite",
    "build": "rm -rf ./dist && vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --fix --max-warnings 0",
    "preview": "vite preview",
    "prepublishOnly": "npm run build",
    "pub": "npm version patch --force && npm publish"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "sideEffects": [
    "**/*.{css,scss}"
  ],
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "classnames": "^2.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "sass": "^1.75.0",
    "vite": "^5.2.0",
    "vite-plugin-lib-inject-css": "^2.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/petarnenov/cool-props-monitoring.git"
